    λ(a : Type)
→   λ(xss : List (List a))
→   List/build
    a
    (   λ(list : Type)
    →   λ(cons : a → list → list)
    →   λ(nil : list)
    →   List/fold
        (List a)
        xss
        list
        (   λ(xs : List a)
        →   λ(ys : list)
        →   List/fold
            a
            xs
            list
            cons
            ys
        )
        nil
    )
