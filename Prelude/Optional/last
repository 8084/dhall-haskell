let last : ∀(a : Type) → List (Optional a) → Optional a
    =   λ(a : Type)
    →   λ(xs : List (Optional a))
    →   List/fold
        (Optional a)
        xs
        (Optional a)
        (   λ(l : Optional a)
        →   λ(r : Optional a)
        →   Optional/fold a r (Optional a) (λ(x : a) → [x] : Optional a) l
        )
        ([] : Optional a)

in  last
